    "clsx": "^2.1.1",
    "install": "^0.13.0",
    "lucia": "^3.2.2",
    "next": "15.1.3",
    "oslo": "^1.2.1",
    "prisma": "^6.1.0",
    "prizma": "^0.0.22",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
